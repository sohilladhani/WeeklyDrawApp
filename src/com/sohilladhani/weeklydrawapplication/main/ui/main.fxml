<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" prefHeight="400.0" prefWidth="600.0"
            xmlns="http://javafx.com/javafx/8"
            fx:controller="com.sohilladhani.weeklydrawapplication.main.controllers.MainController">
    <children>
        <Button fx:id="customerInfoButton" layoutX="161.0" layoutY="84.0" mnemonicParsing="false"
                onAction="#onCustomerInfoButtonPressed" prefHeight="25.0" prefWidth="278.0"
                text="Customer Information"/>
        <Button fx:id="doWeeklyDrawButton" layoutX="161.0" layoutY="167.0" mnemonicParsing="false"
                onAction="#onDoWeeklyDrawButtonPressed" prefHeight="25.0" prefWidth="278.0"
                text="Do Weekly Draw"/>
        <Button fx:id="weeklyWinnersButton" layoutX="161.0" layoutY="248.0" mnemonicParsing="false"
                onAction="#onWeeklyWinnersButtonPressed" prefHeight="25.0" prefWidth="278.0"
                text="Weekly Winners"/>
    </children>
</AnchorPane>
